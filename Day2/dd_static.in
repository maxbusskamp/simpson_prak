spinsys {
	channels 19F
	nuclei 19F 19F
	dipole 1 2 -2000 0 0 0
}

par {
	crystal_file     		alpha0beta0
	# crystal_file    		zcw28656

	method 					direct			
	# proton_frequency 		400.0e6
	
	start_operator   		I1x+I2x
	detect_operator  		I1p
  
	sw						20000
	variable tsw 			1.0e6/sw
	np						4096
	
	spin_rate        		0
	gamma_angles     		1
	verbose          		0100
}
 
proc pulseq {} {
	global par
  
	acq_block {
		delay $par(tsw)
	}
}

proc main {} {
	global par

	set f [fsimpson]
	faddlb $f 50 0
	fft $f
	fsave $f $par(crystal_file).spe
	fsave $f $par(crystal_file).xy -xreim
	funload $f
}

